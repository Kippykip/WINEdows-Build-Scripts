[com.canonical.indicator.sound]
interested-media-players=[ 'parole' ]

[org.gnome.desktop.background]
picture-uri='file:///usr/share/xfce4/backdrops/Setup.png'

[org.gnome.desktop.interface]
gtk-theme='Chicago95'
icon-theme='Chicago95'
font-name='Microsoft Sans Serif 8'
